{
  "name": "weplay-main",
  "version": "0.1.0",
  "private": true,
  "nyc": {
    "excludeAfterRemap": true,
    "report-dir": "cypress/coverage",
    "all": true,
    "reporter": [
      "text",
      "json",
      "html"
    ],
    "extension": [
      ".ts",
      ".tsx",
      "js",
      "jsx"
    ],
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "src/**/*.jsx",
      "src/**/*.js"
    ],
    "exclude": [
      "src/setupTests.ts",
      "src/**/*.test.tsx",
      "src/**/*.cy.ts",
      "src/**/*.cy.tsx",
      "src/**/*.d.ts",
      "src/reportWebVitals.ts",
      "src/API.ts",
      "src/aws-exports.js",
      "src/graphql",
      "src/models"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "@cypress/webpack-dev-server": "^3.2.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "ag-grid-community": "^30.0.2",
    "ag-grid-react": "^30.0.2",
    "aws-amplify": "^5.0.8",
    "dotenv": "^16.0.3",
    "framer-motion": "^11.2.10",
    "i18next": "^22.5.0",
    "immer": "^9.0.21",
    "jest-environment-jsdom": "^29.4.1",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "moment": "^2.30.1",
    "rc-time-picker": "^3.7.3",
    "react": "^18.2.0",
    "react-datepicker": "^7.0.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-autocomplete": "^2.7.3",
    "react-hook-form": "^7.43.8",
    "react-i18next": "^12.3.1",
    "react-image-crop": "^10.1.4",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.3",
    "react-slider": "^2.0.6",
    "react-toastify": "^9.1.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "build-prod": "npm run build",
    "test": "jest --clearCache && jest",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cy:run:chrome": "cypress run -q --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:webkit": "cypress run --browser webkit",
    "test:e2e": "npm run cy:run:chrome",
    "test:unit": "cypress run --component --browser chrome",
    "test:all": "npm run test:e2e && npm run test:unit",
    "cov:combined": "npm run copy:reports && npm run combine:reports && npm run finalize:combined-report",
    "copy:reports": "(mkdir reports || true) && cp /cypress/coverage/coverage-final.json reports/from-cypress.json && cp coverage/coverage-final.json reports/from-jest.json",
    "combine:reports": "(mkdir .nyc_output || true) && npm nyc merge reports && mv coverage.json .nyc_output/out.json",
    "finalize:combined-report": "npm nyc report --reporter html --reporter text --reporter json-summary --report-dir combined-coverage",
    "cov:reset": "del .nyc_output && del reports && del coverage && del cypress/coverage && del combined-coverage",
    "test:unit:chrome": "cypress run -q --component --browser chrome",
    "coverage:all": "npm run cov:reset && npm run test:unit:chrome"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@bahmutov/cypress-code-coverage": "^2.6.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/webpack-preprocessor": "^5.17.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/cypress": "^1.1.3",
    "@types/google.maps": "^3.52.5",
    "@types/jest": "^27.5.2",
    "@types/react-slider": "^1.3.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-loader": "^9.1.3",
    "cypress": "^12.6.0",
    "cypress-mailosaur": "^2.13.0",
    "daisyui": "^4.12.2",
    "esbuild": "^0.17.8",
    "esbuild-plugin-svgr": "^2.0.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "istanbul": "^0.4.5",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.4.2",
    "jest-watch-typeahead": "^2.2.2",
    "nyc": "^15.1.0",
    "playwright-webkit": "^1.31.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
